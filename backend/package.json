{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "16.13.0",
    "npm": "8.1.0"
  },
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node build/src",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "migrate": "ts-node -r dotenv/config node_modules/.bin/node-pg-migrate -j ts",
    "db:seed": "ts-node -r dotenv/config src/seed.ts"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "faker": "^5.5.3",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "node-pg-migrate": "^6.0.0",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/lodash": "^4.14.177",
    "@types/node": "^14.11.2",
    "gts": "^3.1.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  }
}
